<table class="price-sales">
  <colgroup>
    <col width="7rem" />
    <col width="3rem" />
  </colgroup>
  <thead>
    <tr>
      <th>Price Name</th>
      <th>Current</th>
    </tr>
  </thead>
  <tbody>
    <%@prices.each do |price| %>
      <% if price["ProgramID"] == 100 %>
        <tr>
          <td class="td-ps"><%= price["Name"]%></td>
          <td class="td-ps">$<%= price["Sales"]%></td>
        </tr>
      <%end%>
    <%end%>
  </tbody>
</table>